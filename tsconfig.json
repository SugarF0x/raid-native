{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@screens": ["./screens/"],
      "@screens/*": ["./screens/*"],
      "@components": ["./components/"],
      "@components/*": ["./components/*"],
      "@definitions": ["./definitions/"],
      "@definitions/*": ["./definitions/*"],
      "@utils": ["./utils/"],
      "@utils/*": ["./utils/*"],
      "@consts": ["./consts/"],
      "@consts/*": ["./consts/*"],
      "@classes": ["./classes/"],
      "@classes/*": ["./classes/*"],
    }
  }
}
